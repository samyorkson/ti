   1811                      {
   1812                        stackProfilePro = TRUE;
   1813                      }
   1814          
   1815                      if ( stackProfile == 0 )
   1816                      {
   1817                        //Remove from the list
   1818                        bdb_nwkDescFree(pNwkDesc);
